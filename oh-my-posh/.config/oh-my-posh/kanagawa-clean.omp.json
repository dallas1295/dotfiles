{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bg": "#1F1F28",
    "black": "#16161D",
    "blue": "#7E9CD8",
    "border": "#2A2A37",
    "comment": "#727169",
    "cyan": "#7FB4CA",
    "fg": "#DCD7BA",
    "gray": "#54546D",
    "green": "#98BB6C",
    "indigo": "#9CABCA",
    "magenta": "#957FB8",
    "orange": "#FFA066",
    "peach": "#FFA066",
    "pink": "#D27E99",
    "red": "#E46876",
    "surface": "#181820",
    "violet": "#938AA9",
    "white": "#DCD7BA",
    "yellow": "#E6C384"
  },
  "secondary_prompt": {
    "template": "❯ ",
    "foreground": "cyan"
  },
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "yellow",
    "foreground_templates": ["{{ if .Root }}red{{ end }}"]
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "󰉖",
            "home_icon": "",
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "foreground": "blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ if .RepoName }}{{ .UpstreamIcon }} {{ .HEAD }} {{ if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ end }}",
          "foreground": "green",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ❯",
          "foreground": "yellow",
          "type": "text",
          "style": "plain",
          "foreground_templates": ["{{ if .Root }}red{{ end }}"]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
